{
  "name": "ogarepair-core-backend",
  "version": "0.1.0",
  "description": "Core backend for OgaRepair",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "server:dev": "cross-env NODE_ENV=development nodemon",
    "server:test": "cross-env NODE_ENV=test nodemon",
    "server:prod": "cross-env NODE_ENV=production nodemon",
    "start": "node index.js",
    "build": "tsc",
    "lint:check": "eslint ./**/*.{js,jsx,ts,tsx} --max-warnings=0",
    "lint:fix": "eslint ./**/*.{js,jsx,ts,tsx} --fix",
    "prepare": "husky install",
    "tstify:models": "ts-node scripts/tstify.ts --path src/db/models",
    "tstify:migrations": "ts-node scripts/tstify.ts --path src/db/migrations",
    "tstify:seeders": "ts-node scripts/tstify.ts --path src/db/seeders",
    "generate:types": "graphql-codegen"
  },
  "author": "OgaRepair Team",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.2",
    "@graphql-tools/load": "^7.7.4",
    "@graphql-tools/schema": "^8.5.1",
    "@prisma/client": "^4.2.1",
    "apollo-server-express": "^3.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dataloader": "^2.1.0",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.5.1",
    "express-validator": "^6.14.2",
    "graphql": "^16.5.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-scalars": "^1.18.0",
    "helmet": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "uuid": "^8.3.2",
    "xss": "^1.0.13",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-resolvers": "^2.7.3",
    "@prisma-korea/graphql-schema-generator": "^0.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.2.1",
    "prisma-dbml-generator": "^0.9.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "16.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
